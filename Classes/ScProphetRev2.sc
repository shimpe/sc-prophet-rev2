    connect {
        | device="Rev2", name = "Rev2 MIDI 1"|
        var found = false;
        if (MIDIClient.initialized.not) {
            MIDIClient.init;
        };
        MIDIClient.destinations.do({
            |el, idx|
            if ((el.name.debug("check midi endpoint looking for %".format(name)) == name), {
                ("connecting to midi endpoint "++el.name).postln;
                found = true;
                midi_out = MIDIOut.newByName(device, el.name).latency_(Server.default.latency);
            });
        });
        if (found.not) {
            "WARNING: couldn't find MIDI endpoint. Connect failed!.".postln;
        };
        MIDIIn.connectAll;
    }

    makeNRPNResponder {
        |parmnum = nil, action|
        var list = #[99, 98, 6, 38];
        var seq = Pseq(list, 1).asStream;
        var incomingNum = 0;
        var incomingVal = 0;

        CCResponder({ |src, chan, num, val|
            if(num == seq.next) {
                switch(num)
                { 99 } {
                    incomingNum = incomingNum | (val << 7)
                }
                { 98 } {
                    incomingNum = incomingNum | val
                }
                { 6 } {
                    incomingVal = incomingVal | (val << 7)
                }
                { 38 } {
                    incomingVal = incomingVal | val;
                    if ((incomingNum == parmnum) || (parmnum.isNil) ){
                        action.value(incomingVal, incomingNum);
                    };
                    incomingNum = incomingVal = 0;
                    seq.reset;
                }
            } {
                seq.reset
            };
        }, num: list);
    }

    // test send nrpn
    sendNRPN {
        | number, value, channel=1, verbose = false |
        var cCC_MSB = 99;
        var cCC_LSB = 98;
        var cDATA_MSB = 6;
        var cDATA_LSB = 38;
        var number_msb = number >> 7;
        var number_lsb = number & 127;
        var value_msb = value >> 7;
        var value_lsb = value & 127;
        if (verbose) {
            if (rev2_nrpn.at(number)[\hint].notNil) {
                var explanation = rev2_nrpn.at(number)[\hint][value];
                ("sending NRPN "++number++" (msb: "++number_msb++", lsb: "++number_lsb++") ( "++rev2_nrpn.at(number)[\name]++" ) value "++value++"
(msb: "++value_msb++", lsb: "++value_lsb++") = "++explanation).postln;
            } {
                ("sending NRPN "++number++" (msb: "++number_msb++", lsb: "++number_lsb++") ( "++rev2_nrpn.at(number)[\name]++" ) value "++value++"
(msb: "++value_msb++", lsb: "++value_lsb++")").postln;
            };
        };
        if (midi_out.notNil) {
            midi_out.control(channel, cCC_MSB, number_msb);
            midi_out.control(channel, cCC_LSB, number_lsb);
            midi_out.control(channel, cDATA_MSB, value_msb);
            midi_out.control(channel, cDATA_LSB, value_lsb);
        } /* else */ {
            " NOTE: not sending as midi_out is not initialized yet. Call connect first.".postln;
        };
    }

    select_bank {
        | bank, channel = 1 |
        bank = bank.asInteger;
        if (bank < 0) {
            bank = 0;
        } /*else*/ {
            if (bank > 7) {
                bank = 7;
            };
        };
        this.midi_out.control(channel, 16r20, bank);
    }

    select_patch_on_current_bank {
        | patch, channel=1 |
        patch = patch.asInteger;
        if (patch < 0) {
            patch = 0;
        } /* else */ {
            if (patch > 127) {
                patch = 127;
            };
        };
        this.midi_out.program(channel, patch);
    }

    select_bank_patch {
        | bank, patch, channel = 1|
        this.select_bank(bank);
        this.select_patch_on_current_bank(patch);
    }

    select_patch_by_id {
        | bank = "F2", name="P2", channel = 1|
        var offset = 0;
        var split = bank.splitNumbers;
        var bankname = split[0];
        var banknumber = split[1].asInteger - 1;
        var patchnumber = name.splitNumbers[1].asInteger - 1;
        if (bankname.compare("F", ignoreCase:true)==0) {
            offset = 4;
        };
        this.select_bank_patch(banknumber+offset, patchnumber, channel);

    }

    all_notes_off {
        | channel = 1 |
        this.midi_out.control(channel, 16r7B, 0);
